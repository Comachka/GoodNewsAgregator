@model HomePageModel

@{
    ViewData["Title"] = "Home Page";
}

<div class="fav-articles">
    @foreach (var article in @Model.FavouredArticles)
    {
        await Html.RenderPartialAsync("ArticlePreviewPartial", article);
    }
</div>

@section Scripts
    {
    <script src="js/home-page.js"></script>
    <script src="js/raiting.js" defer></script>
}