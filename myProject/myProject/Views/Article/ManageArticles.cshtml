@model ArticlesWithPaginationModel

<div class="gx-2 text-center">
    <a class="btn btn-primary" asp-action="CreateArticleWithSource" asp-controller="Article">Создать новость</a>
    <a class="btn btn-primary" asp-action="Aggregator" asp-controller="Article">Агрегатор новостей</a>
    <a class="btn btn-primary" asp-action="ManageUsers" asp-controller="Account">Редактирование пользователей</a>
</div>


<div class="row article-list">
    @foreach (var el in Model.ArticlePreviews)
    {
       await Html.RenderPartialAsync("ArticlePreviewPartialManage", el);
    }
</div>

<pagination page-info="Model.PageInfo" page-action="ManageArticles">
    <p>1123123123123123123</p>
</pagination>

